        "/stores": {
            "get": {
                "tags": [
                    "Stores"
                ],
                "summary": "Obtener una lista de tiendas",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "Una lista de tiendas con la cantidad de productos de cada tienda",
